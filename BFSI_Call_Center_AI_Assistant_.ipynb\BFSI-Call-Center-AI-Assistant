        "\n",
        "emi_knowledge = \"\"\"EMI CALCULATION KNOWLEDGE BASE\n",
        "\n",
        "EMI FORMULA:\n",
        "   EMI = [P × R × (1+R)^N] / [(1+R)^N - 1]\n",
        "\n",
        "   Where:\n",
        "   P = Principal Loan Amount\n",
        "   R = Monthly Interest Rate (Annual Rate ÷ 12)\n",
        "   N = Number of Monthly Installments\n",
        "\n",
        "EMI CALCULATION EXAMPLES:\n",
        "\n",
        "1. PERSONAL LOAN EXAMPLE\n",
        "   Amount: ₹5,00,000\n",
        "   Rate: 12% p.a. (1% monthly)\n",
        "   Tenure: 3 years (36 months)\n",
        "\n",
        "   EMI = [5,00,000 × 0.01 × (1.01)^36] / [(1.01)^36 - 1]\n",
        "   EMI = ₹16,607 per month\n",
        "\n",
        "   Total Interest: ₹97,852\n",
        "   Total Payment: ₹5,97,852\n",
        "\n",
        "2. HOME LOAN EXAMPLE\n",
        "   Amount: ₹30,00,000\n",
        "   Rate: 9% p.a. (0.75% monthly)\n",
        "   Tenure: 20 years (240 months)\n",
        "\n",
        "   EMI = ₹26,992 per month\n",
        "\n",
        "   Total Interest: ₹34,78,080\n",
        "   Total Payment: ₹64,78,080\n",
        "\n",
        "3. CAR LOAN EXAMPLE\n",
        "   Amount: ₹8,00,000\n",
        "   Rate: 9.5% p.a. (0.7917% monthly)\n",
        "   Tenure: 5 years (60 months)\n",
        "\n",
        "   EMI = ₹16,809 per month\n",
        "\n",
        "   Total Interest: ₹2,08,540\n",
        "   Total Payment: ₹10,08,540\n",
        "\n",
        "4. EDUCATION LOAN EXAMPLE\n",
        "   Amount: ₹15,00,000\n",
        "   Rate: 10% p.a. (0.8333% monthly)\n",
        "   Tenure: 10 years (120 months)\n",
        "   Moratorium: 2 years (course + 1 year)\n",
        "\n",
        "   EMI after moratorium: ₹19,814 per month\n",
        "\n",
        "   Interest during moratorium: Simple interest\n",
        "   Interest amount: ₹3,00,000 (if accrued)\n",
        "\n",
        "EMI FACTORS:\n",
        "1. Interest Rate Impact\n",
