        "main_code.append(\"                \")\n",
        "main_code.append(\"                if result[\\\"params\\\"]:\")\n",
        "main_code.append(\"                    print(f\\\"📊 Detected parameters: {result['params']}\\\")\")\n",
        "main_code.append(\"\")\n",
        "main_code.append(\"            except KeyboardInterrupt:\")\n",
        "main_code.append(\"                print(\\\"\\\\n\\\\n👋 Goodbye!\\\")\")\n",
